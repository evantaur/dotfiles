bindsym alt+Tab exec $HOME/.config/sway/scripts/tab.py
bindsym alt+shift+Tab exec $HOME/.config/sway/scripts/tab.py reverse
bindsym $mod+less exec $HOME/.config/sway/scripts/tab.py scratchpad
